{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "useBuiltIns": "usage"
      }
    ],
    "react",
    "jest"
  ],
  "env": {
    "test": {
      "presets": ["react"],
      "plugins": [
        "transform-es2015-modules-commonjs",
        ["@babel/plugin-proposal-optional-chaining"],
        ["@babel/plugin-proposal-nullish-coalescing-operator"]
      ]
    }
  }
}
